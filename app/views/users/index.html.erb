<h1>Users</h1>

<div id="users">
  <table>
    <thead>
      <tr>
        <th>Usuario</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr> 
          <td>
            <%= user.username %>
          </td>
        <% end %>
      </tr>
    </tbody>
  </table>
</div>
